#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_011F4330 .scope module, "testbench" "testbench" 2 3;
 .timescale -9 -9;
v011F4E18_0 .var "i", 2 0;
v011F4E70_0 .var/i "ip", 31 0;
v011F8B10_0 .net "y", 7 0, v011F4DC0_0; 1 drivers
S_011F43B8 .scope module, "behav" "Decoder_3to8_behav" 2 7, 3 1, S_011F4330;
 .timescale 0 0;
v011F8788_0 .net "i", 2 0, v011F4E18_0; 1 drivers
v011F4DC0_0 .var "y", 7 0;
E_011F6F28 .event edge, v011F8788_0;
    .scope S_011F43B8;
T_0 ;
    %wait E_011F6F28;
    %set/v v011F4DC0_0, 0, 8;
    %load/v 8, v011F8788_0, 3;
    %cmpi/u 8, 0, 3;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 3;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 3;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 3;
    %jmp/1 T_0.3, 6;
    %cmpi/u 8, 4, 3;
    %jmp/1 T_0.4, 6;
    %cmpi/u 8, 5, 3;
    %jmp/1 T_0.5, 6;
    %cmpi/u 8, 6, 3;
    %jmp/1 T_0.6, 6;
    %cmpi/u 8, 7, 3;
    %jmp/1 T_0.7, 6;
    %set/v v011F4DC0_0, 2, 8;
    %jmp T_0.9;
T_0.0 ;
    %ix/load 0, 0, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.1 ;
    %ix/load 0, 1, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.2 ;
    %ix/load 0, 2, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.3 ;
    %ix/load 0, 3, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.4 ;
    %ix/load 0, 4, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.5 ;
    %ix/load 0, 5, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.6 ;
    %ix/load 0, 6, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.7 ;
    %ix/load 0, 7, 0;
    %set/x0 v011F4DC0_0, 1, 1;
    %jmp T_0.9;
T_0.9 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_011F4330;
T_1 ;
    %vpi_call 2 10 "$dumpfile", "Decoder_3to8.vcd";
    %vpi_call 2 11 "$dumpvars", 1'sb0, S_011F4330;
    %set/v v011F4E70_0, 0, 32;
T_1.0 ;
    %load/v 8, v011F4E70_0, 32;
   %cmpi/s 8, 8, 32;
    %jmp/0xz T_1.1, 5;
    %load/v 8, v011F4E70_0, 3; Only need 3 of 32 bits
; Save base=8 wid=3 in lookaside.
    %ix/load 0, 0, 0;
    %set/x0 v011F4E18_0, 8, 1;
    %ix/load 0, 1, 0;
    %set/x0 v011F4E18_0, 9, 1;
    %ix/load 0, 2, 0;
    %set/x0 v011F4E18_0, 10, 1;
    %delay 10, 0;
    %vpi_call 2 19 "$display", "i[2]=%b,i[1]=%b,i[0]=%b,y=%b", &PV<v011F4E18_0, 2, 1>, &PV<v011F4E18_0, 1, 1>, &PV<v011F4E18_0, 0, 1>, v011F8B10_0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v011F4E70_0, 32;
    %set/v v011F4E70_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 22 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "Decoder_3to8_behav.v";
